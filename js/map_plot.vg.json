{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "height": 500,
    "width": 800,
    "title": "World Happiness Report 2019",
    "projection": {"type": "equalEarth"},
    "layer": [
        {
            "data": {
                "url": "./ne_110m.json",
                "format": {"type": "topojson", "feature": "ne_110m"}
            },
            "mark": {
                "type": "geoshape",
                "fill": "lightgray",
                "stroke": "black"
            }
        },
        {
            "data": {
                "url": "../data/world_happiness_report_2019.csv"
            },
            "mark": {
                "type": "circle"
            }
        }
    ]
}